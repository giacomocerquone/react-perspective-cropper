{
  "name": "react-perspective-cropper",
  "homepage": ".",
  "version": "0.2.1",
  "description": "React component performing crop, border detection, perspective correction and simple image filters over a provided image ðŸ“² ðŸ“¸",
  "author": "giacomocerquone",
  "license": "MIT",
  "repository": "giacomocerquone/react-perspective-cropper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs && cp ./src/index.d.ts ./dist/index.d.ts",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "react-scripts": "3.4.1",
    "standard-version": "^9.1.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "opencv-react": "git@github.com:RakeshYadvanshi/opencv-react.git#e669637055cd5a5f73de67433de654146bd25355",
    "prop-types": "^15.7.2",
    "react-draggable": "^4.4.3"
